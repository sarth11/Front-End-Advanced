$(document).ready(function(){
      $(window).scroll(function() { // check if scroll event happened
        if ($(document).scrollTop() > 550) { // check if user scrolled more than 50 from top of the browser window
          $(".move").css("background-color", "#A6A9A6"); // if yes, then change the color of class "navbar-fixed-top" to white (#f8f8f8)
        } else {
          $(".move").css("background-color", "transparent"); // if not, change it back to transparent
        }
      });
    });
